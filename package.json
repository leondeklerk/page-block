{
	"name": "vue3-template",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "bunx --bun vite",
		"build": "bun run lint && bun run type-check && bun run build-only",
		"build:watch": "bunx --bun vite build --watch",
		"build-only": "bunx --bun vite build",
		"lint": "eslint --ext .ts,.vue,.js --max-warnings 0 .",
		"lint:fix": "eslint --ext .ts,.vue,.js --fix .",
		"prepare": "husky",
		"preview": "bunx --bun vite preview",
		"test:unit": "bunx --bun vitest",
		"type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false "
	},
	"lint-staged": {
		"*.{js,ts,vue}": "eslint --ext .ts,.vue,.js --max-warnings 0"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "6.5.2",
		"vue": "3.4.31",
		"xss": "1.0.15",
		"zod": "3.23.8"
	},
	"devDependencies": {
		"@rushstack/eslint-patch": "1.10.3",
		"@tailwindcss/forms": "0.5.7",
		"@tsconfig/node20": "20.1.4",
		"@types/bun": "1.1.6",
		"@types/firefox-webext-browser": "120.0.4",
		"@vitejs/plugin-vue": "5.0.5",
		"@vue/eslint-config-prettier": "9.0.0",
		"@vue/eslint-config-typescript": "13.0.0",
		"@vue/tsconfig": "0.5.1",
		"autoprefixer": "10.4.19",
		"eslint": "8.57.0",
		"eslint-plugin-vue": "9.26.0",
		"husky": "9.0.11",
		"lint-staged": "15.2.7",
		"postcss": "8.4.39",
		"prettier": "3.3.2",
		"tailwindcss": "3.4.4",
		"typescript": "5.5.3",
		"vite": "5.3.2",
		"vue-tsc": "2.0.24"
	}
}
